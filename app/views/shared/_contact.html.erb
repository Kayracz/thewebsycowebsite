
<!-- CONTACT US-->
<div class="container" id="contact">


  <div class="contactform">
      <div class="contactus">
        <h2><%= t "homes.contactus"%></h2>
      </div>

      <div class="paragraphcontact">
        <p><%= t "homes.wewould"%></p>
      </div>

        <div class="text-center buttonemail">
          <button id="form-send-button" data-i18n="contactUs.form.sendButton" class="section-btn form-btn buttonscontact" type="button"><%= link_to 'Email', new_message_path %></button>
        </div>
</div>
</div>


